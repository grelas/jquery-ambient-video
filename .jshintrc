// http://jshint.com/docs/options/

{
  "node": true,
  "browser": true,
  "esnext": true,
  "bitwise": false,
  "camelcase": false,
  "curly": false,
  "eqeqeq": true,
  "immed": true,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": false,
  "strict": true,
  "trailing": true,
  "globals": {
    "jasmine": true,
    "angular": false,
    "ApplicationConfiguration": false,
    "_": true,
    "it" : true,
    "xit" : true,
    "xdescribe": true,
    "fdescribe": true,
    "fit": true,
    "$": true,
    "jQuery": true
  },
  "predef": [
    "define",
    "require",
    "exports",
    "module",
    "describe",
    "before",
    "beforeEach",
    "after",
    "afterEach",
    "it",
    "inject",
    "expect",
    "context",
    "sinon",
    "spyOn",
    "protractor",
    "browser",
    "by",
    "element",
    "braintree",
    "FastClick"
  ],
  "devel": true,
  "noempty": true,
  "laxcomma": true,
  "expr": true
}
